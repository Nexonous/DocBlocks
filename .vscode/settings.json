{
	"cSpell.words": [
    "arrowseperator",
    "docblocks",
    "topbar",
    "vseperator"
  ],
	"editor.tabSize": 2,
	"editor.insertSpaces": true,
	"eslint.validate": [
		"javascript",
		"javascriptreact",
		"html",
		"typescriptreact"
	],
	"editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.autopep8"
  },
  "python.formatting.provider": "none",
}